{
  "expo": {
    "name": "Antera Chat",
    "slug": "antera-chat",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "anterachat",
    "jsEngine": "hermes",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#10B981"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.antera.chat"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#10B981"
      },
      "package": "com.antera.chat",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "dbc1b45b-2823-4fe9-ae3c-ffa497ef986c"
      }
    }
  }
}
