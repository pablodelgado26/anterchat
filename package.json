    "@react-navigation/stack": "^6.3.20",
    "axios": "^1.6.0",
    "date-fns": "^3.0.0",
    "expo": "~51.0.0",
    "expo-image-picker": "^17.0.10",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-image-picker": "^7.1.0",
    "react-native-safe-area-context": "4.10.5",
